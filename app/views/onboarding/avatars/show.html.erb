<p>
  <%= @user.first_name %>
  <%= @user.last_name %>
  <%= @user.location %>
</p>
<p>
  <%= link_to "Not you? Correct your info", onboarding_infos_path  %>



<%= simple_form_for(@user, url: onboarding_avatar_path(@user)) do |f| %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.submit "Upload your picture and go to Your bio" %>
<% end %>
