<div class="user-card-container">


  <div class="top-part">

      <%= image_tag user.photo_url, class: "user-card-avatar" %>


      <div class="user-box" >
        <h4 class="user-box-heading">
        <%= user.first_name %> </h4>
       <h5 class="user-box-title"> <%= user.title %> </h5>

      </div>
  </div>

  <div class="bottom-part">

      <p class="goal-description-user">
        <%= user.goal_description %>
      </p>

      <p class="goal-description-user">
        <%= user.goal_list %>
      </p>

  </div>

    <div class="skill-part">
      <div class="skills-user-list">

        <span> <%= user.skill_list %> </span>

      </div>
    </div>
 </div>

    <div class="match-container">

      <%= link_to "#", class: "disliker" do  %>
        <i class="fas fa-times fa-2x" style= ></i>
      <% end %>

      <button class="liker">
        <i class="fas fa-check fa-2x" ></i>
      </button>

    </div>







