  <%= @user.first_name %>
  <%= @user.last_name %>
  <%= @user.location %>

  <%= @user.photo %>

  <%= link_to "Not you? Correct your info", onboarding_avatars_path %>


<%= simple_form_for(@user, url: onboarding_bio_path(@user)) do |f| %>
    <%= f.input :description,
                as: :text,
                label: false,
                input_html: {
                  value: f.object.description,
                  placeholder: "Tell us about yourself",
                }
    %>
    <%= f.input :achievement,
                as: :text,
                label: false,
                input_html: {
                  value: f.object.achievement,
                  placeholder: "Your most important achievement",
                }
    %>
    <%= f.submit "Proceed to skills" %>
<% end %>
